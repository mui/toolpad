{
  "name": "@toolpad/studio-components",
  "version": "0.6.0",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/toolpad#readme",
  "license": "MIT",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Button": {
      "types": "./dist/Button.d.ts",
      "default": "./dist/Button.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "github:mui/toolpad",
    "directory": "packages/toolpad-studio-components"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit",
    "build:types": "tsc --declaration --emitDeclarationOnly"
  },
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "dependencies": {
    "@mui/icons-material": "6.1.1",
    "@mui/lab": "6.0.0-beta.10",
    "@mui/material": "6.1.1",
    "@mui/x-charts": "7.17.0",
    "@mui/x-data-grid-premium": "7.17.0",
    "@mui/x-date-pickers": "7.17.0",
    "@mui/x-license": "7.17.0",
    "@tanstack/react-query": "5.56.2",
    "@toolpad/studio-runtime": "workspace:*",
    "@toolpad/utils": "workspace:*",
    "dayjs": "1.11.13",
    "invariant": "2.2.4",
    "markdown-to-jsx": "7.4.7",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.52.2"
  },
  "devDependencies": {
    "@types/invariant": "2.2.37",
    "@types/react": "18.3.5",
    "react": "18.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
