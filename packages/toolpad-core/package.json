{
  "name": "@toolpad/core",
  "version": "0.13.0",
  "author": "Toolpad Team",
  "description": "Dashboard framework powered by MUI.",
  "main": "./node/index.js",
  "module": "./index.js",
  "keywords": [
    "react",
    "mui",
    "toolpad",
    "internal tools",
    "crud",
    "admin",
    "dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/toolpad.git",
    "directory": "packages/toolpad-core"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "homepage": "https://github.com/mui/toolpad#readme",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "pnpm clean",
    "build": "pnpm build:node && pnpm build:modern && pnpm build:stable && pnpm build:types && pnpm build:copy-files",
    "build:node": "node ../../scripts/build.mjs node",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "build:types": "tsx ../../scripts/buildTypes.mts",
    "predev": "pnpm clean",
    "dev": "mkdir -p build && concurrently \"pnpm build:stable --watch\" \"pnpm build:types --watch\" \"pnpm build:copy-files\"",
    "check-types": "pnpm build:stable && pnpm build:types && tsc --noEmit",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "test:browser": "vitest run -c vitest.config.browser.mts",
    "test:browser:dev": "vitest -c vitest.config.browser.mts"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "@mui/utils": "^7.0.0",
    "@standard-schema/spec": "^1.0.0",
    "@toolpad/utils": "workspace:*",
    "@vitejs/plugin-react": "4.3.4",
    "client-only": "^0.0.1",
    "dayjs": "1.11.13",
    "invariant": "2.2.4",
    "path-to-regexp": "6.3.0",
    "prop-types": "15.8.1"
  },
  "devDependencies": {
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/x-data-grid": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-data-grid-premium": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-data-grid-pro": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-date-pickers": "^8.0.0 || ^8.0.0-beta",
    "@types/invariant": "2.2.37",
    "@types/prop-types": "15.7.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/sinon": "^17.0.4",
    "@vitest/browser": "2.1.9",
    "next": "^15.2.3",
    "next-router-mock": "^0.9.13",
    "playwright": "^1.47.2",
    "react-router": "7.1.5",
    "sinon": "^19.0.5",
    "vitest": "2.1.9"
  },
  "peerDependencies": {
    "@mui/icons-material": "^7.0.0-beta || ^7.0.0",
    "@mui/material": "^7.0.0-beta || ^7.0.0",
    "@mui/x-data-grid": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-data-grid-premium": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-data-grid-pro": "^8.0.0 || ^8.0.0-beta",
    "@mui/x-date-pickers": "^8.0.0 || ^8.0.0-beta",
    "next": "^14 || ^15",
    "react": "^18 || ^19",
    "react-router": "^7"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react-router": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
