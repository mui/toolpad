{
  "name": "@toolpad/studio-runtime",
  "version": "0.13.0",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/toolpad#readme",
  "license": "MIT",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "public",
    "server",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/toolpad.git",
    "directory": "packages/toolpad-studio-runtime"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "dependencies": {
    "@auth/core": "0.38.0",
    "@mui/material": "6.4.7",
    "@tanstack/react-query": "5.69.0",
    "@toolpad/utils": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@webcontainer/env": "1.1.1",
    "cookie": "1.0.2",
    "fractional-indexing": "3.2.0",
    "invariant": "2.2.4",
    "nanoid": "5.1.5",
    "react-error-boundary": "4.0.13",
    "react-is": "^19.0.0"
  },
  "devDependencies": {
    "@types/cookie": "0.6.0",
    "@types/express": "5.0.0",
    "@types/invariant": "2.2.37",
    "@types/react": "^19.0.10",
    "@types/react-is": "^19.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vm-browserify": "1.1.2"
  },
  "peerDependenciesMeta": {
    "vm-browserify": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
