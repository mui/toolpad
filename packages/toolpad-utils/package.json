{
  "name": "@toolpad/utils",
  "version": "0.8.0",
  "description": "Shared utilities used by Toolpad packages.",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/toolpad#readme",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./events": {
      "types": "./dist/events.d.ts",
      "require": "./dist/events.cjs",
      "default": "./dist/events.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    },
    "./hooks/useBoolean": {
      "types": "./dist/hooks/useBoolean.d.ts",
      "require": "./dist/hooks/useBoolean.cjs",
      "default": "./dist/hooks/useBoolean.js"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "require": "./dist/hooks/*.cjs",
      "default": "./dist/hooks/*.js"
    }
  },
  "files": [
    "dist",
    "server",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/toolpad.git",
    "directory": "packages/toolpad-studio-utils"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit",
    "test": "vitest run --coverage"
  },
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "invariant": "2.2.4",
    "prettier": "3.3.3",
    "react-is": "18.3.1",
    "title": "3.5.3",
    "yaml": "2.5.1",
    "yaml-diff-patch": "2.0.0"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/invariant": "2.2.37",
    "@types/react": "18.3.11",
    "@types/react-is": "18.3.0",
    "@types/title": "3.4.3",
    "vitest": "2.1.3"
  }
}
